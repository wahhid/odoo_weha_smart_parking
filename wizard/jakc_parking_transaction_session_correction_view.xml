<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>               
        <record id="view_parking_transaction_session_correction_form" model="ir.ui.view">
            <field name="name">parking.transaction.session.correction.form</field>
            <field name="model">parking.transaction.session.correction</field>
            <field name="arch" type="xml">
                <form string="Parking Transaction Session Correction Form" version="7.0">                    
                    <group>                    	
                    	<field name="parking_transaction_session_id"/>
                    	<field name="session_date"/>	                        
                    </group>
                    <footer>
                        <button name="trans_correction" string="Submit" type="object" default_focus="1" class="oe_highlight"/>
                        or
                        <button string="Cancel" class="oe_link" special="cancel"/>
                    </footer>
                    
                </form>
            </field>
        </record>                              		               
    </data>
</openerp>