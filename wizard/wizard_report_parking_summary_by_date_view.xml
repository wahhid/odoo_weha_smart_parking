<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_wizard_parking_summary_by_date_form" model="ir.ui.view">
        <field name="name">wizard.parking.summary.by.date.form</field>
        <field name="model">wizard.parking.summary.by.date</field>
        <field name="arch" type="xml">
            <form string="Parking Summary By Date">
                <group>
                    <field name="start_date"/>
                    <field name="end_date"/>
                </group>
                <footer>
                    <button name="generate_report" string="Print" type="object" class="btn-primary"/>
                    <button string="Cancel" class="btn-default" special="cancel" />
                </footer>
            </form>
        </field>
    </record>
</odoo>