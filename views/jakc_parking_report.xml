<odoo>
    <report id="parking_session_summary_report"
            string="Parking Session Summary"
            model="report.jakc_parking.report_parkingsessionsummary"
            report_type="qweb-pdf"
            name="jakc_parking.report_parkingsessionsummary"
            file="jakc_parking.report_parkingsessionsummary"
            menu="False"
     />

    <record id="paperformat_parking_session_summary" model="report.paperformat">
            <field name="name">Parking Session Summary</field>
            <field name="default" eval="True"/>
            <field name="format">custom</field>
            <field name="page_height">297</field>
            <field name="page_width">210</field>
            <field name="orientation">Portrait</field>
            <field name="margin_top">15</field>
            <field name="margin_bottom">10</field>
            <field name="margin_left">10</field>
            <field name="margin_right">10</field>
            <field name="header_line" eval="False"/>
            <field name="header_spacing">10</field>
            <field name="dpi">90</field>
    </record>

