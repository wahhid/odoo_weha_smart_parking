<odoo>
    <data>
        <record id="action_parking_dashboard" model="ir.actions.client">
            <field name="name">Dashboard</field>
            <field name="tag">parking_dashboard</field>
        </record>

        <menuitem name="Dashboard" id="parking_dashboard" parent="parking_main_menu"
                  sequence="1"
                  groups="group_parking_manager"
                  action="action_parking_dashboard"/>
    </data>

    <template id="assets_backend_dashboard" name="POS Dashboard assets inherit" inherit_id="web.assets_backend">
        <xpath expr="." position="inside">
            <script type="text/javascript" src="/weha_smart_parking/static/src/js/dashboard.js"/>
            <link rel="stylesheet" href="/weha_smart_parking/static/src/css/dashboard.css"/>
        </xpath>
    </template>
</odoo>
